

public class FizzBuzDriver {

	public static void main(String[] args) {
		Random rand = new Random(1001);
		rand.nextInt();
		
for (int i = 1; i <= 1000; i++) {
	
	
	if (i % 3 ==0 ) {
		
		System.out.print("Fizz");
		
	}
	
	else if (i % 5 ==0)
	{
		
		System.out.print("Buzz");
		
		
	}
	
	else if (i % 5 ==0) && (i % 3 == 0)
	{
		
		System.out.print("FizzBuzz");
		
		
	}
